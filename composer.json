{
    "name": "nanotronic/doctrine-solr",
    "description": "Solr and Doctrine integration",
    "keywords": ["search", "solr"],
    "homepage": "https://github.com/nanotronic/doctrine-solr",
    "type": "library",
    "require": {
        "php": ">=5.4.3",
        "doctrine/annotations": ">=1.1.0",
        "doctrine/inflector": "1.0.*@dev",
        "doctrine/orm": ">=2.4.0",
        "symfony/property-access": ">=2.4.0",
        "jms/metadata": "dev-master",
        "solarium/solarium": "3.*"
    },
    "minimum-stability": "dev",
    "dev-require": {
    },
    "autoload": {
        "psr-0": {
            "Doctrine\\Solr": "lib/"
        }
    },
     "extra": {
        "branch-alias": {
            "dev-master": "2.2.x-dev",
            "dev-2.1": "2.1.x-dev",
            "dev-2.0": "2.0.x-dev",
            "dev-1.0": "1.0.x-dev"
        }
    }
}
